<h1>Tank Page</h1>

<% @tanks.each do |tank| %>
  <p><%= tank.tank_type %><%= tank.number %><br><%= tank.status%></p>
  <%= image_tag('fv.png') %>
<% end %><br>
<div class="d-inline-block">
  <div class='card'>
    <%= render "brew_update_form" %>
  </div>
</div>

<div class="d-inline-block">
  <div class='card'>
    <%= render "cellar_update_form" %>
  </div>
</div>

<div class="d-inline-block">
  <div class='card'>
    <%= render "transfer_update_form" %>
  </div>
</div>


<div class="d-inline-block">
  <div class='card'>
    <%= render "package_update_form" %>
  </div>
</div>

<div class="d-inline-block">
  <div class='card'>
    <%= render "overide_form" %>
  </div>
</div>
<p>* put BBT before BBT Number, ex: BBT5</p>
