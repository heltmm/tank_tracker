
<%= form_for @tank, :url => {:action => "overide"}, :html => {:id => "overide_form"}, :method => "patch", remote: true do |f|%>
  <p>Overide Tank</p>
  <%= f.select :tank_type, [["FV", "FV"],["BBT", "BBT"]] %>
  <%= f.label(:number, "Number") %>
  <%= f.number_field(:number, :required => true) %><br>
  <%= f.label(:volume, "Volume") %>
  <%= f.number_field(:volume) %><br>
  <%= f.label(:brand, "Brand") %>
  <%= f.text_field(:brand) %><br>
  <%= f.label(:gyle, "Gyle") %>
  <%= f.number_field(:gyle) %><br>


  <%= f.submit("Update") %>
<% end %>
